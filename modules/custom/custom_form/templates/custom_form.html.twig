<form action="/afip_form/add" type="POST">
	<fieldset>
		<legend>Informacion Personal:</legend>
		<label for="nombre">Nombre:</label>
		<input type="text" id="nombre" name="nombre" value="">
		<label for="apellido">Apellido:</label>
		<input type="text" id="apellido" name="apellido" value="">
		<label for="fecha de nacimiento">Fecha de nacimiento:</label>
		<label for="dni">DNI:</label>
		<input type="number" id="dni" name="dni" value="">
		<label for="cuit">CUIT:</label>
		<input type="number" id="cuit" name="cuit" value="">
		<label for="estado civil">Estado Civil:</label>
		<input type="radio" id="soltero" name="estado civil" value="soltero">
        <label for="soltero">Soltero</label><br>
        <input type="radio" id="casado" name="estado civil" value="casado">
        <label for="casado">Casado</label><br>
		<label for="hijos">Hijos:</label>
		<input type="number" id="hijos" name="hijos" value="">

		<fieldset>
			<legend>Genero:</legend>
			<label for="male"><input class="radio_gender" type="radio" id="male" name="gender" value="male"> Masculino</label>
            <label for="female"><input type="radio" id="female" name="gender" value="female"> Femenino</label>
			<label for="other"><input type="radio" id="other" name="gender" value="other"> Otro</label>
			<label for="otro_genero">Especifique cual:</label>
			<input type="text" id="otro_genero" name="otro_genero" value="">
		</fieldset>
	</fieldset>
	<fieldset>
		<legend>Direccion:</legend>
		<label for="pais">Pais:</label>
		<select id="pais" name="pais">
			<option value="ar">Argentina</option>
			<option value="br">Brazil</option>
			<option value="ch">Chile</option>
			<option value="ur">Uruguay</option>
		</select>
		<label for="provincia">Provincia:</label>
		<select id="provincia" name="provincia">
			<option value="ar">Buens Aires</option>
			<option value="br">Santa fe</option>
			<option value="ch">Cordoba</option>
			<option value="ur">San Luis</option>
		</select>
		<label for="localidad">Localidad:</label>
		<input type="text" id="localidad" name="localidad" value="">
		<label for="calle">Calle:</label>
		<input type="text" id="calle" name="calle" value="">
		<label for="numero">Numero:</label>
		<input type="number" id="numero" name="numero" value="">
		<label for="piso">Piso:</label>
		<input type="number" id="piso" name="piso" value="">
		<label for="codigo postal">Codigo Postal:</label>
		<input type="number" id="codigo postal" name="codigo postal" value="">
	</fieldset>
	<fieldset>
	    <legend>Datos de contacto:</legend>
		<label for="email">E-mail:</label>
		<input type="email" id="email" name="email">
		<label for="telefono celular">Telefono celular:</label>
		<input type="number" id="telefono celular" name="telefono celular" value="">
		<label for="telefono fijo">Telefono fijo:</label>
		<input type="number" id="telefono fijo" name="telefono fijo" value="">
	</fieldset>
	<fieldset>
	    <label for="notificaciones">多Desea recibir notificaciones?:</label>
		<label for="terminos y condiciones">多Acepta los ternimos y condiciones?:</label>

	</fieldset>
	<fieldset>
	    <label for="user">Usuario:</label>
		<input type="text" id="user" name="user" value="">
		<label for="password">Contrase単a:</label>
		<input type="password" id="password" name="password">
	</fieldset>

	<input type="submit" value="Submit">
</form>

