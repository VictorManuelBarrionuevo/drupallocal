<form action="/form_insert">
	<label for="nombre">Nombre:</label><br>
	<input type="text" id="nombre" name="nombre" placeholder="Juan"><br>

	<label for="apellido">Apellido:</label><br>
	<input type="text" id="apellido" name="apellido" placeholder="Perez"><br><br>
	<div>
		<label for="nacimiento">Nacimiento:</label>
		<input type="date" id="nacimiento" name="nacimiento">
	</div>
	<div>
		<label for="dni">Numero de DNI:</label>
		<input type="number" id="dni" name="dni" placeholder="12698577">
	</div>
	<div>
		<label for="cuit">Numero de CUIT:</label>
		<input type="number" id="cuit" name="cuit" placeholder="20-12698577-3">
	</div>
	<fieldset>
		<legend>Estado Civil</legend>
		<div>
			<input type="radio" id="Casado" name="estado_civil" value="0">
			<label for="Casado">Casado</label>
			<input type="radio" id="Soltero" name="estado_civil" value="1"> 
			<label for="Soltero">Soltero</label>
			<input type="radio" id="Otros" name="estado_civil" value"2">
			<label for="Otros">Otros</label>
		</div>
	</fieldset>
	<div>
		<label for="hijos">Hijos:</label>
		<input type="number" id="hijos" name="hijos">
	</div>

	<fieldset>
		<legend>Genero:</legend>
		<div>
			<input type="radio" id="Femenino" name="genero" value="0">
			<label for="Femenino">Femenino</label>
			<input type="radio" id="Masculino" name="genero" value="1">
			<label for="Masculino">Masculino</label>
			<input type="radio" id="Otros" name="genero" value="2">
			<label for="Otros">Otros</label>
		</div>
	</fieldset>

	<fieldset>
	<legend>Ubicacion</legend>
		<div id="dropdown">
			<label for="pais">Pais:</label>
			<select id="pais" name="pais"></select>
		</div>

		<div class="ocultar">
			<label for="provincia">Provincia:</label>
			<select id="provincia" name="provincia"></select>
		</div>
		<div id='pais_prov' style="display:none;">
			{% for var in vars.paises %}
				<li attr-name="{{ var.name }}" attr-id="{{ var.id }}"></li>
			{% endfor %}
		</div>
		<div>
			<label for="localidad">Localidad:</label>
			<input type="text" id="localidad" name="localidad">
		</div>
		<div>
			<label for="calle">Calle:</label>
			<input type="text" id="calle" name="calle">
		</div>
		<div>
			<label for="numero">Numero:</label>
			<input type="text" id="numero" name="numero">
		</div>
		<div>
			<label for="piso">Piso:</label>
			<input type="number" id="piso" name="piso">
		</div>
		<div>
			<label for="codigo_postal">Codigo Postal:</label>
			<input type="text" id="codigo_postal" name="codigo_postal">
		</div>
	</fieldset>
	<fieldset>
		<legend>Informacion de contacto:</legend>
		<div>
			<label for="email">Email:</label>
			<input type="text" id="email" name="email" placeholder="email@example.com">
		</div>

		<div>
			<label for="telefono_celular">Telefono Celular:</label>
			<input type="numer" id="telefono_celular" name="telefono_celular" placeholder="261-85974">
		</div>
		<div>
			<label for="telefono_fijo">Telefono Fijo:</label>
			<input type="numer" id="telefono_fijo" name="telefono_fijo" placeholder="000 222 1111">
		</div>

	</fieldset>

	<input type="submit" value="Submit">
</form>


