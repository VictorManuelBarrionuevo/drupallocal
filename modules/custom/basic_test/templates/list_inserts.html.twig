{#{ dump(vars) }#}
<div>
	<a id="" class="add_button" href="/add_new">Add New</a>
</div>

<div class="form_list">
	<table>
		<tr>
			<th>IDENTIFICADOR</th>
			<th>NOMBRE</th>
			<th>ACCIONES</th>
		</tr>
		{% for var in vars.listado %}
			<tr>
				<td>{{ var.id }}</td>
				<td><a href="">{{ var.nombre }} {{ var.apellido }}</a></td>
				<td>
				<a href="">EDITAT</a>
				<a href="">DELETE</a> 
				</td>
			</tr>
		{% endfor %}
	</table>
</div>

{% for page in vars.pages %}
	<a href='/?page={{page}}'>{{page}}</a>
{% endfor %}
